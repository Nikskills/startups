generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Startup {
  id String @id @default(uuid())
  title String 
  slug String
  description String
  author String
  views String
  category String
  pitch String
  image String
}

model User {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
  username String
  password String
}

